{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://admin.hillway.in/api/:path*"
    },
    {
      "source": "/admin",
      "destination": "https://admin.hillway.in/admin"
    },
    {
      "source": "/admin/:path*",
      "destination": "https://hilway7.vercel.app/admin/:path*"
    },
    {
      "source": "/_next/:path*",
      "destination": "https://admin.hillway.in/_next/:path*"
    },
    
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
