{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://aapka-backend-project.vercel.app/api/:path*"
    },
    {
      "source": "/admin",
      "destination": "https://aapka-backend-project.vercel.app/admin"
    },
    {
      "source": "/admin/:path*",
      "destination": "https://aapka-backend-project.vercel.app/admin/:path*"
    },
    {
      "source": "/_next/:path*",
      "destination": "https://aapka-backend-project.vercel.app/_next/:path*"
    }
  ]
}